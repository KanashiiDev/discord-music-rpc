{
  "name": "discord-music-rpc",
  "version": "0.6.0",
  "description": "Discord Music RPC Application",
  "homepage": "https://github.com/KanashiiDev/discord-music-rpc",
  "main": "main.js",
  "keywords": [
    "discord",
    "electron",
    "rpc",
    "rich presence",
    "extension",
    "music",
    "radio",
    "server",
    "tray"
  ],
  "author": {
    "name": "KanashiiDev",
    "email": "kanashiidev0@gmail.com",
    "url": "https://github.com/KanashiiDev"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.11.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:app": "electron .",
    "build:win": "node scripts/generateAsarUnpack.js win && electron-builder --win --x64 --publish=never",
    "build:linux": "node scripts/generateAsarUnpack.js linux && electron-builder --linux --x64 --publish=never --config.extraMetadata.productName='discord-music-rpc'",
    "build:mac": "node scripts/generateAsarUnpack.js mac && electron-builder --mac universal --publish=never",
    "build:chrome": "cross-env TARGET=chrome node scripts/buildExtensions.js",
    "build:firefox": "cross-env TARGET=firefox node scripts/buildExtensions.js",
    "build:extensions": "npm run build:chrome && npm run build:firefox",
    "pack": "electron-builder --dir",
    "pack:chrome": "cross-env TARGET=chrome node scripts/packExtensions.js",
    "pack:firefox": "cross-env TARGET=firefox node scripts/packExtensions.js",
    "pack:extensions": "npm run pack:chrome && npm run pack:firefox",
    "build-and-pack": "npm run build:extensions && npm run pack:extensions",
    "prepare-release": "npm run build-and-pack && node scripts/prepareRelease.js"
  },
  "dependencies": {
    "@xhayper/discord-rpc": "^1.3.0",
    "cors": "^2.8.5",
    "electron-log": "^5.4",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0",
    "semver": "^7.7.3",
    "simple-json-db": "^2.0.0"
  },
  "devDependencies": {
    "acorn": "^8.15.0",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.1.0",
    "electron": "^39.2.0",
    "electron-builder": "^26.0.18",
    "fs-extra": "^11.3.2",
    "postcss": "^8.5.6"
  },
  "build": {
    "appId": "com.kanashiidev.discord.music.rpc",
    "productName": "Discord Music RPC",
    "copyright": "Copyright Â© 2025 KanashiiDev",
    "publish": [
      {
        "provider": "github",
        "owner": "KanashiiDev",
        "repo": "discord-music-rpc"
      }
    ],
    "asar": true,
    "compression": "maximum",
    "directories": {
      "output": "dist/${platform}"
    },
    "win": {
      "artifactName": "Discord-Music-RPC-Setup-${version}.exe",
      "target": "nsis",
      "icon": "assets/icon/icon.ico"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "discord-music-rpc-${version}.AppImage",
      "executableName": "discord-music-rpc",
      "category": "Audio;Music",
      "icon": "assets/icon/",
      "maintainer": "KanashiiDev <kanashiidev0@gmail.com>",
      "synopsis": "Show music from ANY website on your Discord!",
      "description": "Discord Music RPC Application - Show music from ANY website on your Discord!",
      "desktop": {
        "entry": {
          "Name": "Discord Music RPC",
          "Comment": "Show music from ANY website on your Discord!",
          "Keywords": "discord;music;rpc;presence;",
          "Categories": "Audio;Music;",
          "Icon": "assets/icon/icon.png"
        }
      }
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "artifactName": "Discord-Music-RPC-${version}-mac.dmg",
      "icon": "assets/icon/icon.icns",
      "darkModeSupport": true,
      "extendInfo": {
        "LSUIElement": true
      }
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "allowToChangeInstallationDirectory": true,
      "shortcutName": "${productName}",
      "installerSidebar": "assets/installer/sidebar.bmp",
      "installerIcon": "assets/icon/icon.ico",
      "uninstallDisplayName": "${productName}",
      "uninstallerIcon": "assets/icon/icon.ico"
    },
    "files": [
      "main.js",
      "scripts/electron-log.js",
      "utils.js",
      "server.js",
      "LICENSE",
      "assets/**/*",
      "package.json"
    ],
    "extraFiles": [
      "LICENSE"
    ],
    "asarUnpack": [
      "assets/icon/**/*",
      "node_modules/@discordjs/collection/**",
      "node_modules/@discordjs/rest/**",
      "node_modules/@discordjs/util/**",
      "node_modules/@sapphire/async-queue/**",
      "node_modules/@sapphire/snowflake/**",
      "node_modules/@vladfrangu/async_event_emitter/**",
      "node_modules/@xhayper/discord-rpc/**",
      "node_modules/accepts/**",
      "node_modules/argparse/**",
      "node_modules/body-parser/**",
      "node_modules/builder-util-runtime/**",
      "node_modules/bytes/**",
      "node_modules/call-bind-apply-helpers/**",
      "node_modules/call-bound/**",
      "node_modules/content-disposition/**",
      "node_modules/content-type/**",
      "node_modules/cookie-signature/**",
      "node_modules/cookie/**",
      "node_modules/cors/**",
      "node_modules/debug/**",
      "node_modules/depd/**",
      "node_modules/discord-api-types/**",
      "node_modules/dunder-proto/**",
      "node_modules/ee-first/**",
      "node_modules/electron-log/**",
      "node_modules/electron-updater/**",
      "node_modules/encodeurl/**",
      "node_modules/es-define-property/**",
      "node_modules/es-errors/**",
      "node_modules/es-object-atoms/**",
      "node_modules/escape-html/**",
      "node_modules/etag/**",
      "node_modules/express/**",
      "node_modules/finalhandler/**",
      "node_modules/forwarded/**",
      "node_modules/fresh/**",
      "node_modules/fs-extra/**",
      "node_modules/function-bind/**",
      "node_modules/get-intrinsic/**",
      "node_modules/get-proto/**",
      "node_modules/gopd/**",
      "node_modules/graceful-fs/**",
      "node_modules/has-symbols/**",
      "node_modules/hasown/**",
      "node_modules/http-errors/**",
      "node_modules/iconv-lite/**",
      "node_modules/inherits/**",
      "node_modules/ipaddr.js/**",
      "node_modules/is-promise/**",
      "node_modules/js-yaml/**",
      "node_modules/jsonfile/**",
      "node_modules/lazy-val/**",
      "node_modules/lodash.escaperegexp/**",
      "node_modules/lodash.isequal/**",
      "node_modules/magic-bytes.js/**",
      "node_modules/math-intrinsics/**",
      "node_modules/media-typer/**",
      "node_modules/merge-descriptors/**",
      "node_modules/mime-db/**",
      "node_modules/mime-types/**",
      "node_modules/ms/**",
      "node_modules/negotiator/**",
      "node_modules/object-assign/**",
      "node_modules/object-inspect/**",
      "node_modules/on-finished/**",
      "node_modules/once/**",
      "node_modules/parseurl/**",
      "node_modules/path-to-regexp/**",
      "node_modules/proxy-addr/**",
      "node_modules/qs/**",
      "node_modules/range-parser/**",
      "node_modules/raw-body/**",
      "node_modules/router/**",
      "node_modules/safe-buffer/**",
      "node_modules/safer-buffer/**",
      "node_modules/sax/**",
      "node_modules/semver/**",
      "node_modules/send/**",
      "node_modules/serve-static/**",
      "node_modules/setprototypeof/**",
      "node_modules/side-channel-list/**",
      "node_modules/side-channel-map/**",
      "node_modules/side-channel-weakmap/**",
      "node_modules/side-channel/**",
      "node_modules/simple-json-db/**",
      "node_modules/statuses/**",
      "node_modules/tiny-typed-emitter/**",
      "node_modules/toidentifier/**",
      "node_modules/tslib/**",
      "node_modules/type-is/**",
      "node_modules/undici/**",
      "node_modules/universalify/**",
      "node_modules/unpipe/**",
      "node_modules/vary/**",
      "node_modules/wrappy/**",
      "node_modules/ws/**",
      "server.js",
      "utils.js"
    ],
    "afterPack": "./scripts/buildCleanup.js"
  }
}